<p style="color: green"><%= notice %></p>

<h1>Friends</h1>

<div id="friends">
  <% @friends.each do |friend| %>
    <%= render partial: 'friend', object: friend %>
    <p>
      <%= link_to "Show this friend", profile_path(friend.id) %>
    </p>
  <% end %>
</div>

<%= link_to "New friend", new_friend_path %>
